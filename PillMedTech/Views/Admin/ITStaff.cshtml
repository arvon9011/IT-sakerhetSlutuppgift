@model IEnumerable<Logger>;

<nav>
  <p>
    <a asp-controller="Account" asp-action="Login">Login</a> / <strong>Loggfil</strong>
    <a asp-controller="Account" asp-action="Logout" class="right">Logga ut</a>
  </p>
</nav>

<main>
  <h2>Loggfiler</h2>

  <table>
    <tr>
      <th>Datum</th>
      <th>AnställningsID</th>
      <th>IP-adress</th>
      <th>Händelse</th>
    </tr>
    @*TODO: Vilken poco-klass har du för loggningen? Lägg till modellen ovan och byt ut frågetecknet. Ta sedan bort denna rad*@
    @foreach(Logger logg in Model)
    {
      <tr>
        <td>@logg.Time</td>
        <td>@logg.EmployeeId</td>
        <td>@logg.Ip</td>
        <td>@logg.Action</td>
      </tr>
    }
  </table>
</main>
